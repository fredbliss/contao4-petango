{
  "name": "intelligentspark/contao4-petango",
  "description": "Consume petango api into static json & display pet details.",
  "type": "contao-bundle",
  "keywords": ["contao","petango"],
  "authors": [
    {
      "name": "Intelligent Spark"
    }
  ],
  "require": {
    "php": ">=7.0",
    "contao/core-bundle": "^4.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "IntelligentSpark\\Petango\\": "src/"
    }
  },
  "classmap": [
    "src/Resources/contao/"
  ],
  "exclude-from-classmap": [
    "src/Resources/contao/config/",
    "src/Resources/contao/dca/",
    "src/Resources/contao/languages/",
    "src/Resources/contao/templates/"
  ],
  "extra": {
    "contao-manager-plugin": "IntelligentSpark\\Petango\\ContaoManager\\Plugin"
  }
}